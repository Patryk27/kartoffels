  "eh-frame-header": false,
  "emit-debug-gdb-scripts": false,
  "features": "+a,+m,+unaligned-scalar-mem",
  "linker": "rust-lld",
  "linker-flavor": "gnu-lld",
  "llvm-abiname": "ilp32",
  "llvm-target": "riscv32",
  "max-atomic-width": 32,
  "metadata": {
    "description": null,
    "host_tools": null,
    "std": null,
    "tier": null
  },
  "panic-strategy": "abort",
  "relocation-model": "static",
  "singlethread": true,
  "supported-sanitizers": [],
  "target-pointer-width": "32"
}
